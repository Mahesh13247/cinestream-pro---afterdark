    "@google/genai": "^1.30.0",
    "@tanstack/react-query": "^5.90.11",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "cheerio": "^1.1.2",
    "framer-motion": "^12.23.24",
    "hls.js": "^1.6.15",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
